    <string name="sharing_pattern__help">
        Followed expressions will be replaced with appropriate values in runtime:\n\n\n
        <b>{title}</b> - Series title\n\n
        <b>{link}</b> - Series link\n\n
        <b>{score}</b> - Your score\n\n
        <!--<b>{tags}</b> - Your tags\n\n-->
        <b>{watched_episodes}</b> - Your watched episodes (e.g 4)\n\n
        <b>{watched_episodes?}</b> - Your watched episodes with number suffix (e.g 4th)\n\n
        <b>{read_chapters}</b> - Your read chapters (e.g 4)\n\n
        <b>{read_chapters?}</b> - Your read chapters with number suffix (e.g 4th)\n\n
        <b>{read_volumes}</b> - Your read volumes (e.g 4)\n\n
        <b>{read_volumes?}</b> - Your read volumes with number suffix (e.g 4th)\n\n
        <b>{series_episodes}</b> - Series episodes\n\n
        <b>{series_chapters}</b> - Series chapters\n\n
        <b>{series_volumes}</b> - Series volumes
    </string>

